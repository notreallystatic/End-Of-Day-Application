  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.4.1",
    "cloudinary": "^1.21.0",
    "concurrently": "^4.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.16.4",
    "fuzzysort": "^1.1.4",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.3.0",
    "moment": "^2.24.0",
    "mongoose": "^5.3.11",
    "multer": "^1.4.2",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "path": "^0.12.7",
    "react-bootstrap": "^1.0.0",
    "validator": "^10.9.0"
  },
  "engines": {
    "node": "12.14.0"
  }
